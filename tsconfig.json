{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@contracts/*": [
        "./src/contracts/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@shared/*": [
        "./src/shared/*"
      ],
      "@interfaces/*": [
        "./src/shared/interfaces/*"
      ],
      "@services/*": [
        "./src/shared/services/*"
      ],
      "@helpers/*": [
        "./src/shared/helpers/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
